{
  "schemaVersion": 1,
  "id": "sentinel",
  "version": "${version}",
  "name": "Sentinel",
  "description": "Keygun2001's addon pack for Meteor Client",
  "authors": [
    "keygun-development"
  ],
  "contact": {
    "repo": "https://github.com/keygun-development/Sentinel"
  },
  "icon": "assets/template/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "com.keygun2001.addon.Sentinel"
    ]
